            <font>
                <Font name="System Bold" size="18.0" />
            </font>
        </Label>
        <RadioButton fx:id="inHouseRadio" layoutX="123.0" layoutY="50.0" mnemonicParsing="false" onAction="#setInHouseLabel" prefHeight="25.0" prefWidth="138.0" text="In-House">
            <font>
                <Font size="16.0" />
            </font>
         <toggleGroup>
            <ToggleGroup fx:id="modPartToggleGroup" />
         </toggleGroup>
        </RadioButton>
        <RadioButton fx:id="outsourcedRadio" layoutX="261.0" layoutY="50.0" mnemonicParsing="false" onAction="#setOutsourcedLabel" prefHeight="25.0" prefWidth="138.0" text="Outsourced" toggleGroup="$modPartToggleGroup">
            <font>
                <Font size="16.0" />
            </font>
        </RadioButton>
        <AnchorPane layoutX="38.0" layoutY="90.0" prefHeight="328.0" prefWidth="365.0">
            <children>
                <AnchorPane layoutY="14.0">
                    <children>
                        <TextField fx:id="modPartIDTextField" disable="true" layoutX="98.0" prefHeight="37.0" prefWidth="158.0" promptText="Enter Part ID">
                            <font>
                                <Font name="System Italic" size="16.0" />
                            </font>
                        </TextField>
                        <Label layoutY="6.0" prefHeight="20.0" prefWidth="38.0" text="ID">
                            <font>
                                <Font name="System Bold" size="16.0" />
                            </font>
                        </Label>
                    </children>
                </AnchorPane>
                <AnchorPane layoutX="-1.0" layoutY="68.0">
                    <children>
                        <TextField fx:id="modPartNameTextField" disable="true" layoutX="100.0" prefHeight="37.0" prefWidth="158.0" promptText="Enter Part Name">
                            <font>
                                <Font name="System Italic" size="16.0" />
                            </font>
                        </TextField>
                        <Label layoutY="6.0" prefHeight="25.0" prefWidth="53.0" text="Name">
                            <font>
                                <Font name="System Bold" size="16.0" />
                            </font>
                        </Label>
                    </children>
                </AnchorPane>
                <AnchorPane layoutX="-2.0" layoutY="113.0">
                    <children>
                        <TextField fx:id="modPartInventoryTextField" disable="true" layoutX="100.0" prefHeight="37.0" prefWidth="158.0" promptText="Enter Inv. Level">
                            <font>
                                <Font name="System Italic" size="16.0" />
                            </font>
                        </TextField>
                        <Label layoutY="6.0" prefHeight="25.0" prefWidth="79.0" text="Inventory">
                            <font>
                                <Font name="System Bold" size="16.0" />
                            </font>
                        </Label>
                    </children>
                </AnchorPane>
                <AnchorPane layoutX="-1.0" layoutY="162.0">
                    <children>
                        <TextField fx:id="modPartPriceTextField" disable="true" layoutX="100.0" prefHeight="37.0" prefWidth="158.0" promptText="Enter Price">
                            <font>
                                <Font name="System Italic" size="16.0" />
                            </font>
                        </TextField>
                        <Label layoutY="6.0" prefHeight="25.0" prefWidth="79.0" text="Price">
                            <font>
                                <Font name="System Bold" size="16.0" />
                            </font>
                        </Label>
                    </children>
                </AnchorPane>
                <AnchorPane layoutY="214.0" prefHeight="37.0" prefWidth="256.0">
                    <children>
                        <TextField fx:id="modPartInvMaxTextField" disable="true" layoutX="100.0" prefHeight="37.0" prefWidth="66.0" promptText="Max">
                            <font>
                                <Font name="System Italic" size="16.0" />
                            </font>
                        </TextField>
                        <TextField fx:id="modPartInvMinTextField" disable="true" layoutX="190.0" prefHeight="37.0" prefWidth="66.0" promptText="Min">
                            <font>
                                <Font name="System Italic" size="16.0" />
                            </font>
                        </TextField>
                        <Label layoutY="6.0" prefHeight="25.0" prefWidth="79.0" text="Inv. Level">
                            <font>
                                <Font name="System Bold" size="16.0" />
                            </font>
                        </Label>
                    </children>
                </AnchorPane>
                <AnchorPane layoutY="265.0">
                    <children>
                        <TextField fx:id="modPartChangedTextField" disable="true" layoutX="98.0" prefHeight="37.0" prefWidth="158.0">
                            <font>
                                <Font name="System Italic" size="16.0" />
                            </font>
                        </TextField>
                        <Label fx:id="modPartLabelChanger" layoutY="6.0" prefHeight="25.0" prefWidth="95.0">
                            <font>
                                <Font name="System Bold" size="16.0" />
                            </font>
                        </Label>
                    </children>
                </AnchorPane>
            </children>
        </AnchorPane>
        <Button fx:id="modPartCancelButton" layoutX="337.0" layoutY="418.0" mnemonicParsing="false" onAction="#setModPartCancelButton" prefHeight="37.0" prefWidth="66.0" text="Cancel">
            <font>
                <Font size="14.0" />
            </font>
        </Button>
        <Button fx:id="modPartSaveButton" layoutX="250.0" layoutY="418.0" mnemonicParsing="false" onAction="#setModPartSaveButton" prefHeight="37.0" prefWidth="66.0" text="Save">
            <font>
                <Font size="14.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
